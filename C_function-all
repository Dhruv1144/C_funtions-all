#include<stdio.h>
void add(int a,int b)
{
	printf("\nAdd = %d",a+b);
}
void sub(int a,int b)
{
	printf("\nSub = %d",a-b);
}
void mult(int a,int b)
{
	printf("\nMult = %d",a*b);
}
void cube(int a)
{
	printf("\nCube = %d",a*a*a);
}
main()
{
	int a,b;
	printf("\n Enter two numbers =>");
	scanf("%d %d",&a,&b);
	add(a,b);
	cube(a);
	mult(a,b);
	sub(a,b);
	add(a,b);
}
